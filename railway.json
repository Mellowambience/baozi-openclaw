{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "claim-alert-agent/Dockerfile",
    "dockerContextPath": "claim-alert-agent"
  },
  "deploy": {
    "startCommand": "node dist/agent.js",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 3
  }
}
